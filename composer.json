{
  "name": "structuremedia/laravel-versio",
  "description": "Laravel 8.x Versio package",
  "keywords": [
    "structuremedia",
    "dns",
    "versio"
  ],
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Jo Hooyberghs",
      "email": "jo@structuremedia.be",
      "role": "Owner"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "guzzlehttp/guzzle": ">7.0"
  },
  "autoload": {
    "psr-4": {
      "Structuremedia\\Versio\\": "src/"
    }
  },
  "extra": {
      "laravel": {
          "providers": [
              "Structuremedia\\Versio\\VersioServiceProvider"
          ],
          "aliases": {
            "Versio": "Structuremedia\\Versio\\Facades\\Versio"
        }
      }
  }
}
